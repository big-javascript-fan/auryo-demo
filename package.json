  "version": "2.1.2",
  "description": "Listen to SoundCloudÂ® from the comfort of your desktop. Use keyboard shortcuts to navigate through your music. Be more productive.",
  "main": "./dist/main/index",
  "scripts": {
    "parcel:bundle-script": "node ./tools/parcel-bundler.js",
    "electron:start:with-url": "ELECTRON_START_URL=http://localhost:1234 electron .",
    "contributors": "node ./tools/contributors.js",
    "less": "node ./tools/lessc.js",
    "lint:style": "stylelint ./src/less/*.less --fix",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json  'src/**/*{.ts,.tsx}' --fix",
    "lint:tests": "tslint ./tests/**/*.ts{,x} -c tslint.json --fix",
    "lint:templates": "standard ./static/templates/**/*.js",
    "lint": "npm-run-all lint:*",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "parcel:build": "node ./tools/parcel-build.js",
    "parcel:watch": "cross-env NODE_ENV=development node ./tools/parcel-watch.js",
    "publish": "electron-forge publish",
    "start": "rimraf ./dist && electron-forge start",
    "tsc": "node ./tools/tsc.js",
    "electron-releases": "node ./tools/fetch-releases.js",
    "lint-fix": "npm run lint -- --fix",
    "dev": "env-cmd .env concurrently \"npm run watch-renderer\" \"npm run watch-main\"",
    "watch-renderer": "webpack-dev-server --config webpack.config.renderer.dev.js  --watch --mode development",
    "watch-main": "webpack --config webpack.config.main.js --watch --mode development",
    "build-renderer": "webpack --config webpack.config.renderer.prod.js",
    "build-main": "webpack --config webpack.config.main.js --mode production",
    "build": "env-cmd .env concurrently \"npm run build-renderer\" \"npm run build-main\" electron-builder ",
    "build-win": "npm run build && electron-builder -w -p always",
    "build-mac": "npm run build && electron-builder -m -p always",
    "build-linux": "npm run build && electron-builder -l -p always",
    "prebuild": "electron-rebuild",
    "build-test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 npm run build-main && npm run build-renderer",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha test/e2e/**.spec.js test/e2e/**/*.spec.js --compilers js:babel-core/register -R spec-xunit-file --full-trace ",
    "package:dir": "npm run build && build --dir",
    "package:win": "build --win --x64",
    "package:mac": "build --mac",
    "package:linux": "build --linux",
    "package:all": "build -mwl",
    "release": "build -wl --ia32 --x64 --publish onTagOrDraft",
    "release:win": "build -w --ia32 --x64 --publish onTagOrDraft",
    "release:linux": "build -l --x64 --publish onTagOrDraft",
    "release:mac": "build -m --x64 --publish onTagOrDraft",
    "cleanup": "mop -v"
  },
  "config": {
    "forge": "./forge.config.js"
  },
